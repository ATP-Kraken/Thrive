    <AnimationDefinition name="MoveToMenuButtonD0" duration="0.5" replayMode="once">

        <Affector property="Position" interpolator="UVector2" applicationMethod="absolute">

            <KeyFrame position="0.0" progression="linear" sourceProperty="Position" />

            <KeyFrame position="0.3" progression="quadratic acceleration" value="{{0.0, 0}, {1.0, -105}}" />

        </Affector>

        <Affector property="Alpha" interpolator="float" applicationMethod="absolute">

            <KeyFrame position="0.0" progression="linear" value="1.0" />

            <KeyFrame position="0.3" progression="quadratic acceleration" value="0.0" />

        </Affector>

    </AnimationDefinition>

