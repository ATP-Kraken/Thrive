    <AnimationDefinition name="rotate180right" duration="3.0" replayMode="once">

        <Affector property="Rotation" interpolator="QuaternionSlerp" applicationMethod="relative">

            <KeyFrame position="0.0" progression="linear" value="x:0 y:0 z:0" />

            <KeyFrame position="3.0" progression="linear" value="x:0 y:0 z:180" />

        </Affector>

    </AnimationDefinition>

    <AnimationDefinition name="rotate180left" duration="0.8" replayMode="once">

        <Affector property="Rotation" interpolator="QuaternionSlerp" applicationMethod="relative">

            <KeyFrame position="0.0" progression="linear" value="0" />

            <KeyFrame position="0.45" progression="quadratic decelerating" value="-180" />

        </Affector>

    </AnimationDefinition>

    <AnimationDefinition name="fadeout" duration="2.5" replayMode="once">

        <Affector property="Alpha" interpolator="float" applicationMethod="absolute">

            <KeyFrame position="0.0"  progression="quadratic acceleration" value="1.0" />

            <KeyFrame position="2.5" progression="quadratic acceleration" value="0.0" />

        </Affector>

    </AnimationDefinition>


    <AnimationDefinition name="fadein" duration="2.5" replayMode="once">

        <Affector property="Alpha" interpolator="float" applicationMethod="absolute">

            <KeyFrame position="0.0"  progression="quadratic acceleration" value="0.0" />

            <KeyFrame position="2.5" progression="quadratic acceleration" value="1.0" />

        </Affector>

    </AnimationDefinition>
